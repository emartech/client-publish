{
  "name": "@emartech/client-publish",
  "description": "Deployer for client side projects.",
  "main": "index.js",
  "bin": {
    "client-deploy": "./bin/deploy.js",
    "client-deploy-staging": "./bin/deploy-staging.js",
    "client-deploy-production": "./bin/deploy-production.js",
    "client-merge": "./bin/merge.js"
  },
  "scripts": {
    "code-style": "eslint '**/*.js'",
    "test": "mocha setup-tests.spec.js lib --recursive",
    "semantic-release": "CI=true semantic-release pre && npm publish --access public && semantic-release post"
  },
  "release": {
    "verifyConditions": {
      "path": "./node_modules/semantic-release/src/lib/plugin-noop.js"
    }
  },
  "author": "Emarsys",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emartech/client-publish.git"
  },
  "bugs": {
    "url": "https://github.com/emartech/client-publish/issues"
  },
  "homepage": "https://github.com/emartech/client-publish#readme",
  "engines": {
    "node": ">=6.9.0"
  },
  "dependencies": {
    "child-process-promise": "2.2.1",
    "glob": "7.1.2",
    "inquirer": "3.2.1",
    "mime": "1.3.6",
    "rename": "1.0.4",
    "s3": "4.4.0",
    "superagent": "3.5.2",
    "yargs": "8.0.2"
  },
  "devDependencies": {
    "chai": "4.1.1",
    "co-mocha": "1.2.0",
    "eslint": "4.4.1",
    "eslint-config-emarsys": "4.0.0",
    "mocha": "3.5.0",
    "mock-fs": "4.4.1",
    "semantic-release": "7.0.2",
    "sinon": "3.2.0",
    "sinon-chai": "2.13.0"
  }
}
